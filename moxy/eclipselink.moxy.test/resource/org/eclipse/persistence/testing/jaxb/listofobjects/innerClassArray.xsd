   <xsd:complexType name="myInnerArray">
      <xsd:sequence>
         <xsd:element name="item" type="x:myInner" nillable="true" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>  
