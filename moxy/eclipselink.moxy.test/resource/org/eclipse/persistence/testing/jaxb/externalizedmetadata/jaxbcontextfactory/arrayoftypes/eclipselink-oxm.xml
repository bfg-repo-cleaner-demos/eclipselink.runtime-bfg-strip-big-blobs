<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm">
	<java-types>
		<java-type name="org.eclipse.persistence.testing.jaxb.externalizedmetadata.jaxbcontextfactory.arrayoftypes.Employee">
  			<xml-root-element name="anEmployee"/>
  			<java-attributes>
  				<xml-transient java-attribute="lastName"/>
  			</java-attributes>
		</java-type>
		<java-type name="org.eclipse.persistence.testing.jaxb.externalizedmetadata.jaxbcontextfactory.arrayoftypes.Address">
  			<xml-root-element name="anAddress"/>
            <java-attributes>
                <xml-element java-attribute="id" required="true"/>
                <xml-element java-attribute="state" type="java.lang.String"/>
            </java-attributes>
		</java-type>
	</java-types>
</xml-bindings>