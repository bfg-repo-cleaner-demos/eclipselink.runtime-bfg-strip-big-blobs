<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="listOfObjectsNamespace" xmlns:x="listOfObjectsNamespace" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:complexType name="listofObjects">
      <xsd:sequence>
         <xsd:element name="empList" type="x:employee" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="empTreeSet" type="x:employee" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="empArrayList" type="x:employee" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="integerList" type="xsd:int" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="empArray" type="xsd:anySimpleType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="integerArray" type="xsd:anySimpleType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="floatArray" type="xsd:anySimpleType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="intArray" type="xsd:anySimpleType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="booleanArray" type="xsd:anySimpleType" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element name="stringIntegerHashMap" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="entry" minOccurs="0" maxOccurs="unbounded">
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="key" type="xsd:string" minOccurs="0"/>
                           <xsd:element name="value" type="xsd:int" minOccurs="0"/>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="employee">
      <xsd:sequence>
         <xsd:element name="firstName" type="xsd:string" minOccurs="0"/>
         <xsd:element name="lastName" type="xsd:string" minOccurs="0"/>
         <xsd:element name="birthday" type="xsd:dateTime" minOccurs="0"/>
         <xsd:element name="responsibilities" minOccurs="0">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="responsibility" type="xsd:anySimpleType" minOccurs="0" maxOccurs="unbounded"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="id" type="xsd:int" use="required"/>
   </xsd:complexType>
   <xsd:element name="employee-data" type="x:employee"/>
</xsd:schema>
