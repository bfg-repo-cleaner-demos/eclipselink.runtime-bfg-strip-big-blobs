<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm">
    <xml-schema>
        <xml-ns namespace-uri="http://www.example.com/cars" prefix="c"/>
        <xml-ns namespace-uri="http://www.example.com/trucks" prefix="t"/>
    </xml-schema>
	<java-types>
		<java-type name="org.eclipse.persistence.testing.jaxb.externalizedmetadata.mappings.direct.Car">
            <java-attributes>
                <xml-element java-attribute="horsepower" xml-path="features/c:horsepower" />
            </java-attributes>
		</java-type>
        <java-type name="org.eclipse.persistence.testing.jaxb.externalizedmetadata.mappings.direct.Truck">
            <java-attributes>
                <xml-element java-attribute="torque" xml-path="features/t:torque" />
            </java-attributes>
        </java-type>
	</java-types>
</xml-bindings>