Export-Package: org.eclipse.persistence*;version="${version.string}";-noimport:=true,
Implementation-Version: ${version.string}
Specification-Vendor: ${specification.vendor}
Bundle-Name: EclipseLink JPA
Bundle-RequiredExecutionEnvironment: J2SE-1.5
HK2-Bundle-Name: org.eclipse.persistence:org.eclipse.persistence.jpa
Require-Bundle: org.eclipse.persistence.core;bundle-version="${version.string}";visibility:=reexport, \
 org.eclipse.persistence.asm;bundle-version="${version.string}";resolution:=optional
Implementation-Vendor: ${implementation.vendor}
Bundle-Vendor: ${implementation.vendor}
Bundle-Version: ${version.string}
Bundle-Activator: org.eclipse.persistence.jpa.osgi.Activator
Include-Resource:\
 ${jpa.2.base.dir}/about.html,\
 ${jpa.2.base.dir}/readme.html,\
 ${jpa.2.base.dir}/license.html,\
 org/eclipse/persistence/jpa=resource/org/eclipse/persistence/jpa,\
 META-INF/services/javax.persistence.spi.PersistenceProvider=resource/META-INF/services/javax.persistence.spi.PersistenceProvider
Import-Package:                                                         \
 javax.persistence.*;version="[2.0.0, 2.1)",                           \
 javax.sql.*;resolution:=optional,                                      \
 javax.transaction.*;version="1.1.0";resolution:=optional,              \
 javax.xml.*;resolution:=optional,                                      \
 javax.validation.*;resolution:=optional,                               \
 javax.validation.groups.*;resolution:=optional,                        \
 org.apache.tools.ant.*;resolution:=optional,                           \
 org.eclipse.persistence.internal.libraries.asm.*,                      \
 org.eclipse.persistence.internal.libraries.antlr.*,                    \
 org.eclipse.persistence.*;version="${version.string}",                 \
 org.osgi.*;resolution:=optional,                                       \
 org.w3c.*;resolution:=optional,                                        \
 org.xml.sax.*;resolution:=optional, * 
Specification-Version: ${release.version}

