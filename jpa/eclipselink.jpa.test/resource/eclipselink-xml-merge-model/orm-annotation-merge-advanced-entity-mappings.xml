<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_1_0.xsd" version="1.0">
    <entity name = "XMLMergeEmployee" class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Employee" access="PROPERTY" metadata-complete="true">
        <!--table name="CMP3_XML_MERGE_EMPLOYEE"/>
        <secondary-table name="CMP3_XML_MERGE_SALARY"/>
        <primary-key-join-column name="XML_MERGE_EMP_ID" referenced-column-name="XML_MERGE_EMP_ID"/>
        <table-generator name="XML_MERGE_EMPLOYEE_TABLE_GENERATOR" table="CMP3_XML_MERGE_EMPLOYEE_SEQ" pk-column-name="SEQ_NAME" value-column-name="SEQ_COUNT" pk-column-value="XML_MERGE_EMPLOYEE_SEQ"/>
        <named-query name="findAllXMLMergeEmployeesByFirstName" query="SELECT OBJECT(employee) FROM XMLMergeEmployee employee WHERE employee.firstName = :firstname"/>
        <entity-listener class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.EmployeeListener">
            <pre-persist method-name="prePersist"/>
            <post-persist method-name="postPersist"/>
            <pre-remove method-name="preRemove"/>
            <post-remove method-name="postRemove"/>
            <pre-remove method-name="preUpdate"/>
            <post-update method-name="postUpdate"/>
            <post-load method-name="postLoad"/>
        </entity-listener-->
    <attributes>
      <id name="id">
        <column name="XML_MERGE_EMP_ID"/>
        <!--generated-value strategy="TABLE" generator="XML_MERGE_EMPLOYEE_TABLE_GENERATOR"/-->
      </id>
      <!--basic name="firstName">
        <column name="XML_MERGE_F_NAME"/>
      </basic>
      <basic name="lastName">
        <column name="XML_MERGE_L_NAME"/>
      </basic>
      <basic name="salary">
        <column table="CMP3_XML_MERGE_SALARY"/>
      </basic>
      <version name="version">
        <column name="XML_MERGE_VERSION"/>
      </version-->
      <many-to-one name="address" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Address" fetch="LAZY">
        <join-column name="XML_MERGE_ADDR_ID"/>
        <cascade>
          <cascade-persist/>
        </cascade>
      </many-to-one>
      <many-to-one name="manager" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Employee" fetch="LAZY">
        <join-column name="XML_MERGE_MANAGER" referenced-column-name="XML_MERGE_EMP_ID"/>
        <cascade>
          <cascade-persist/>
        </cascade>
      </many-to-one>
      <one-to-many name="phoneNumbers" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.PhoneNumber" mapped-by="owner">
        <cascade>
          <cascade-all/>
        </cascade>
      </one-to-many>
      <one-to-many name="managedEmployees" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Employee" mapped-by="manager">
        <cascade>
          <cascade-all/>
        </cascade>
      </one-to-many>
      <many-to-many name="projects" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Project">
        <cascade>
          <cascade-persist/>
        </cascade>
        <!--order-by>name</order-by>
        <join-table name="CMP3_XML_MERGE_PROJ_EMP">
          <join-column name="XML_MERGE_EMP_ID" referenced-column-name="XML_MERGE_EMP_ID"/>
          <inverse-join-column name="XML_MERGE_PROJ_ID" referenced-column-name="XML_MERGE_PROJ_ID"/>
        </join-table-->
      </many-to-many>
      <embedded name="period">
        <attribute-override name="startDate">
          <column name="XML_MERGE_START_DATE" nullable="false"/>
        </attribute-override>
        <attribute-override name="endDate">
          <column name="XML_MERGE_END_DATE" nullable="true"/>
        </attribute-override>
      </embedded>
    </attributes>
    </entity>
    <entity name="XMLMergeAddress" class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Address" access="PROPERTY" metadata-complete="true">
      <!--table name="CMP3_XML_MERGE_ADDRESS"/>
        <sequence-generator name="XML_MERGE_ADDRESS_SEQUENCE_GENERATOR" sequence-name="XML_MERGE_ADDRESS_SEQ" allocation-size="25"/>
        <named-query name="findAllXMLMergeAddressesByPostalCode" query="SELECT OBJECT(address) FROM XMLMergeAddress address WHERE address.postalCode = :postalcode"/>
        <named-native-query name="findAllXMLMergeAddresses" query="SELECT * FROM CMP3_XML_MERGE_ADDRESS" result-class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Address"/-->
      <attributes>
            <id name="id">
                <column name="XML_MERGE_ADDRESS_ID"/>
                <!--generated-value strategy="SEQUENCE" generator="XML_MERGE_ADDRESS_SEQUENCE_GENERATOR"/-->
            </id>
            <!--basic name="postalCode">
                <column name="XML_MERGE_P_CODE"/>
            </basic>
            <basic name="street"/>
            <basic name="city"/>
            <basic name="province"/>
            <basic name="country"/-->
            <one-to-many name="employees" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Employee" mapped-by="address">
                <cascade>
                  <cascade-all/>
                </cascade>
            </one-to-many>
      </attributes>
    </entity>
    <entity name="XMLMergePhoneNumber" class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.PhoneNumber" access="PROPERTY" metadata-complete="true">
      <!--table name="CMP3_XML_MERGE_PHONENUMBER"/-->
        <id-class class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.PhoneNumberPK"/>
      <attributes>
            <id name="id">
                <column name="XML_MERGE_OWNER_ID" insertable="false" updatable="false"/>
            </id>
            <id name="type">
                <column name="XML_MERGE_TYPE"/>
            </id>
            <basic name="number">
                <column name="XML_MERGE_NUMB"/>
            </basic>
            <!--basic name="areaCode">
                <column name="XML_MERGE_AREA_CODE"/>
            </basic-->
            <many-to-one name="owner" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Employee">
                <join-column name="XML_MERGE_OWNER_ID" referenced-column-name="XML_MERGE_EMP_ID"/>
            </many-to-one>
      </attributes>
    </entity>
    <entity name="XMLMergeLargeProject" class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.LargeProject" access="PROPERTY" metadata-complete="true">
      <table name="CMP3_XML_MERGE_LPROJECT"/>
        <!--discriminator-value>XML_MERGE_L</discriminator-value-->
        <!--named-query name="findXMLMergeLargeProjectWithBudgetLargerThan" query="SELECT OBJECT(project) FROM XMLMergeLargeProject project WHERE project.budget >= :amount"/-->
    </entity>
    <entity name="XMLMergeSmallProject" class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.SmallProject" access="PROPERTY" metadata-complete="true">
      <!--table name="CMP3_XML_MERGE_PROJECT"/>
        <discriminator-value>XML_MERGE_S</discriminator-value-->
    </entity>
    <entity name="XMLMergeProject" class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Project" access="PROPERTY" metadata-complete="true">
      <!--table name="CMP3_XML_MERGE_PROJECT"/-->
        <inheritance strategy="JOINED"/>
        <discriminator-value>XML_MERGE_P</discriminator-value>
        <discriminator-column name="XML_MERGE_PROJ_TYPE" length="0"/>
        <!--sequence-generator name="XML_MERGE_PROJECT_SEQUENCE_GENERATOR" sequence-name="XML_MERGE_PROJECT_SEQ" allocation-size="10"/>
        <named-query name="findXMLMergeProjectByName" query="SELECT OBJECT(project) FROM XMLMergeProject project WHERE project.name = :name"/-->
      <attributes>
            <id name="id">
                <column name="XML_MERGE_PROJ_ID"/>
                <!--generated-value strategy="SEQUENCE" generator="XML_MERGE_PROJECT_SEQUENCE_GENERATOR"/-->
            </id>
            <!--basic name="name">
                <column name="XML_MERGE_PROJ_NAME"/>
            </basic>
            <basic name="description">
                <column name="XML_MERGE_DESCRIP"/>
            </basic>
            <version name="version">
                <column name="XML_MERGE_VERSION"/>
            </version-->
            <one-to-one name="teamLeader" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Employee">
                <join-column name="XML_MERGE_LEADER_ID"/>
            </one-to-one>
            <many-to-many name="teamMembers" target-entity="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.Employee" mapped-by="projects"/>
      </attributes>
        <!--pre-persist method-name="prePersist"/>
        <post-persist method-name="postPersist"/>
        <pre-remove method-name="preRemove"/>
        <post-remove method-name="postRemove"/>
        <pre-remove method-name="preUpdate"/>
        <post-update method-name="postUpdate"/>
        <post-load method-name="postLoad"/-->
    </entity>
    <embeddable class="org.eclipse.persistence.testing.models.jpa.xml.merge.advanced.EmploymentPeriod" access="PROPERTY" metadata-complete="true">
        <!--table name="CMP3_XML_MERGE_EMPLOYEE"/-->
        <attributes>
            <basic name="startDate">
                <column name="XML_MERGE_S_DATE"/>
            </basic>
            <basic name="endDate">
                <column name="XML_MERGE_E_DATE"/>
            </basic>
        </attributes>
    </embeddable>
</entity-mappings>
