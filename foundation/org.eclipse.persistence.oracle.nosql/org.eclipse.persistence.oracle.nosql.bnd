Export-Package: org.eclipse.persistence*;version="${release.version}";-noimport:=true,
Bundle-Name: EclipseLink Oracle NoSQL Extensions
Bundle-RequiredExecutionEnvironment: JavaSE-1.6
HK2-Bundle-Name: org.eclipse.persistence:${ant.project.name}
Bundle-Vendor: ${implementation.vendor}
Fragment-Host: org.eclipse.persistence.nosql;bundle-version="${release.version}"
Bundle-Version: ${version.string}
Include-Resource:\
 ${oracle.nosql.2.base.dir}/about.html,\
 ${oracle.nosql.2.base.dir}/readme.html,\
 ${oracle.nosql.2.base.dir}/license.html
Import-Package:\
 javax.resource.*;version="1.5.0";resolution:=optional,\
 oracle.*;resolution:=optional,\
 org.eclipse.persistence.*;version="${release.version}",\
 org.w3c.*;resolution:=optional,\
 org.xml.*;resolution:=optional,\
 *;resolution:=optional
